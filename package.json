{
  "name": "aestheticize",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "connect-mongo": "^2.0.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "mongoose": "^5.4.5",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "Aestheticize is my 3rd project for the KU Coding Bootcamp. It's a social media site based on aesthetics and personality rather than material image.",
  "main": "server.js",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "ws": "3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelH94/Aestheticize.git"
  },
  "keywords": [],
  "author": "Michael J Haggard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichaelH94/Aestheticize/issues"
  },
  "homepage": "https://github.com/MichaelH94/Aestheticize#readme"
}
